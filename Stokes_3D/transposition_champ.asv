function [Ux_transp,Uy_transp,Uz_transp] = transposition_champ(Ux,Uy,Uz)
%-------------------------------------------------------------------------%
%           transpose le champ par couche de telles maniere 
%               a pouvoir sommer les champs de vitesses
%-------------------------------------------------------------------------%
for z = 1:size(U,3)
    for x = 1:size(U,1)
        U_transp(:,size(U,1) + 1 - x,z) = U(x,:,z)'; 
    end
end
end